{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "node"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "types/**/*.d.ts",
    "interfaces/**/*.d.ts",
    "types/global.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
